{
	"name": "GitHub-Issue-to-Hexo",
	"version": "0.2.2",
	"main": "dist/index.js",
	"repository": "git@github.com:zenghongtu/gh-issue-to-hexo.git",
	"author": "zenghongtu <zenghongtu@gmail.com>",
	"license": "MIT",
	"scripts": {
		"build": "rm -rf dist && ncc build src/index.ts -o dist -m",
		"bump": "yarn run build && git commit -am 'bump'",
		"dev": "tsc -w",
		"postinstall": "patch-package",
		"version:patch": "yarn version --patch",
		"release": "yarn run bump && yarn run version:patch && git push --tags && git push"
	},
	"devDependencies": {
		"@types/hexo": "^3.8.7",
		"@types/node": "^18.0.3",
		"patch-package": "^6.4.7",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@actions/core": "^1.9.0",
		"@actions/github": "^5.0.3",
		"@vercel/ncc": "^0.34.0",
		"hexo": "^6.2.0"
	}
}
